{
  "manifest_version": 3,
  "name": "Hide Animated Emoji",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["*://*.telegram.org/*"],
      "__js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}

